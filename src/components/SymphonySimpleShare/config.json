{
  "comment": "Sample component configuration",
  "components": {
    "SymphonySimpleShare": {
      "window": {
        "url": "$applicationRoot/components/SymphonySimpleShare/SymphonySimpleShare.html",
        "options": {
          "resizable": false,
          "autoShow": false,
          "alwaysOnTop": false
        },
        "top": "center",
        "left": "center",
        "width": 250,
        "height": 250
      },
      "component": {
        "preload": false,
        "spawnOnStartup": true,
        "singleton": true,
        "showDevConsoleOnVisible": false
      },
      "foreign": {
        "services": {
          "windowService": {
            "manageWindowMovement": true,
            "allowAutoArrange": false,
            "allowSnapping": true,
            "allowTabbing": false,
            "allowTiling": false,
            "allowGrouping": false,
            "allowMinimize": false
          },
          "workspace": {}
        },
        "components": {
          "App Launcher": {
            "launchableByUser": true
          },
          "Window Manager": {
            "alwaysOnTopIcon": false,
            "FSBLHeader": {
              "hideMaximize": true,
              "hideMinimize": true,
              "hideClose": true
            },
            "title": "Share To Symphony",
            "persistWindowState": true
          },
          "Toolbar": {
            "iconClass": "SymphonySimpleShare",
            "iconURL": "http://localhost:3375/assets/img/Finsemble_Taskbar_Icon.png"
          }
        }
      }
    }
  }
}
